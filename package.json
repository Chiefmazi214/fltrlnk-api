  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:lifestyle": "ts-node -r tsconfig-paths/register scripts/seed-lifestyle-info.ts",
    "seed:lifestyle:file": "ts-node -r tsconfig-paths/register scripts/seed-lifestyle-info-from-file.ts",
    "setup:roles": "ts-node -r tsconfig-paths/register scripts/setup-roles.ts",
    "validate:business-data": "ts-node -r tsconfig-paths/register scripts/validate-business-data.ts",
    "clear:business-data": "ts-node -r tsconfig-paths/register scripts/clear-business-data.ts",
    "list:businesses": "ts-node -r tsconfig-paths/register scripts/list-existing-businesses.ts",
    "import:businesses": "ts-node -r tsconfig-paths/register scripts/batch-import-businesses.ts",
    "import:businesses:partial": "ts-node -r tsconfig-paths/register scripts/batch-import-businesses.ts",
    "update:business-passwords": "ts-node -r tsconfig-paths/register scripts/update-business-passwords.ts",
    "import:full-businesses-data": "ts-node -r tsconfig-paths/register scripts/import-business-data.ts",
    "update:category": "ts-node -r tsconfig-paths/register scripts/update-business-category.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
